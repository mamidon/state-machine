FETCH0:
	DrPC LdA LdMAR
	goto FETCH1

FETCH1:
	DrMEM LdIR
	goto FETCH2

FETCH2: 
	DrALU LdPC ALUHi ALULo
	dispatch

ADDI0:
	DrREG LdA
	goto ADDI1

ADDI1:
	DrOFF LdB
	goto ADDI2

ADDI2:
	DrALU RegSelLo WrREG
	goto FETCH0

ADD0:
	DrREG RegSelLo LdA
	goto ADD1

ADD1:
	DrREG LdB
	goto ADD2

ADD2:
	DrALU WrREG RegSelHi
	goto FETCH0

NAND0:
	DrREG RegSelLo LdA
	goto NAND1

NAND1:
	DrREG RegSelHi LdB
	goto NAND2

NAND2:
	DrALU ALULo WrREG
	goto FETCH0

LW0:
	DrREG LdA
	goto LW1

LW1:
	DrOFF LdB
	goto LW2

LW2:
	DrALU LdMAR
	goto LW3

LW3:
	DrMEM WrREG RegSelLo
	goto FETCH0

SW0:
	DrREG LdA
	goto SW1

SW1:
	DrOFF LdB
	goto SW2

SW2:
	DrALU LdMAR
	goto SW3

SW3:
	DrREG WrMEM RegSelLo
	goto FETCH0

BEQ0:
	DrREG LdA
	goto BEQ1

BEQ1:
	DrREG RegSelLo LdB
	goto BEQ2

BEQ2:
	DrALU LdZ
	goto BEQ3

BEQ3:
	DrALU
	onz BEQ4 else FETCH0

BEQ4:
	DrPC LdA
	goto BEQ5

BEQ5:
	DrOFF LdB
	goto BEQ6

BEQ6:
	DrALU LdPC
	goto FETCH0

JALR0:
	DrPC WrREG RegSelLo
	goto JALR1

JALR1:
	DrREG LdPC
	goto FETCH0

HALT:
	DrALU
	goto HALT

BONI0:
	DrREG
	goto HALT

BONJ0:
	DrMEM
	goto HALT
BONR0:
	DrPC
	goto HALT
BONO0:
	DrOFF
	goto HALT

